node {
 try  {
 notify('Job Started')


  stage('Git-Checkout') {
   git 'https://github.com/AbhishekPattnaik04/capstone301-A.git'
  }

  stage('Maven-Clean') {
   sh label: '', script: 'mvn clean'
  }

 stage('Maven-Compile') {
   sh label: '', script: 'mvn compile'
  }
  stage('Sonar Analysis'){
        withSonarQubeEnv('SonarQube'){
         sh 'mvn sonar:sonar'
        }
        }

   stage('Maven-Test') {
   sh label: '', script: 'mvn test'
  }


   stage('Maven-Package') {
   sh label: '', script: 'mvn package'
  }

   stage('Jfrog Artifacory Upload') {
        def server= Artifactory.server 'Artifactory'
                    def uploadSpec= """{
                        "files": [{
                        "pattern": "/root/.jenkins/workspace/kalingaMusic/target/*.war",
                        "target": "Kalingamusic"}]
                    }"""
        server.upload(uploadSpec)

        }

notify('Job Completed')

